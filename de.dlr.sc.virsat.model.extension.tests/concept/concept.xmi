      <values name="LOW" value="15"/>
      <values name="MEDIUM" value="20"/>
      <values name="HIGH" value="25"/>
      <values name="INCREDIBLE" value="30"/>
    </properties>
  </categories>
  <categories name="TestCategoryComposition" isApplicableForAll="true">
