		      snippet="de.dlr.sc.virsat.model.extension.sequencing.ui.snippet.UiSnippetTableSequenceEntry">
		</uiSnippet>
		<uiSnippet
		      id="de.dlr.sc.virsat.model.extension.sequencing.section.uiSnippetSequenceEntry"
		      section="de.dlr.sc.virsat.model.extension.sequencing.ui.Section"
		      snippet="de.dlr.sc.virsat.model.extension.sequencing.ui.snippet.UiSnippetSectionSequenceEntry">
		</uiSnippet>
		<uiSnippet
		      id="de.dlr.sc.virsat.model.extension.sequencing.table.uiSnippetSequence"
