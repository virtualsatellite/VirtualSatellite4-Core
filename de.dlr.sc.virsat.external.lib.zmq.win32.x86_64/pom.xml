<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<parent>
		<artifactId>de.dlr.sc.virsat</artifactId>
		<groupId>de.dlr.sc.virsat</groupId>
		<version>4.9.3-SNAPSHOT</version>
		<relativePath>../maven/pom.xml</relativePath>
	</parent>
	<modelVersion>4.0.0</modelVersion>
	<artifactId>de.dlr.sc.virsat.external.lib.zmq.win32.x86_64</artifactId>
	<name>de.dlr.sc.virsat.external.lib.zmq.win32.x86_64</name>
	<packaging>eclipse-plugin</packaging>

	<build>
		<plugins>
			<plugin>
				<groupId>org.eclipse.tycho</groupId>
				<artifactId>target-platform-configuration</artifactId>
				<version>${tycho-version}</version>
				<configuration>
					<environments>
						<environment>
							<os>win32</os>
							<ws>win32</ws>
							<arch>x86_64</arch>
						</environment>
					</environments>
					<target>
						<artifact>
							<groupId>de.dlr.sc.virsat</groupId>
							<artifactId>de.dlr.sc.virsat.target</artifactId>
							<version>4.9.3-SNAPSHOT</version>
							<classifier>virsat</classifier>
						</artifact>
					</target>
				</configuration>
			</plugin>
		</plugins>
	</build>


</project>