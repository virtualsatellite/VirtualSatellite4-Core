		</uiSnippet>
		<uiSnippet
		      id="de.dlr.sc.virsat.model.extension.budget.power.table.uiSnippetPowerSummary"
		      section="de.dlr.sc.virsat.model.extension.budget.power.ui.Section"
		      snippet="de.dlr.sc.virsat.model.extension.budget.power.ui.snippet.UiSnippetTablePowerSummary">
		</uiSnippet>
		<uiSnippet
		      id="de.dlr.sc.virsat.model.extension.budget.power.section.uiSnippetPowerSummary"
		      section="de.dlr.sc.virsat.model.extension.budget.power.ui.Section"
		      snippet="de.dlr.sc.virsat.model.extension.budget.power.ui.snippet.UiSnippetSectionPowerSummary">
		</uiSnippet>
		<uiSnippet
		      id="de.dlr.sc.virsat.model.extension.budget.power.table.uiSnippetPowerState"
		      section="de.dlr.sc.virsat.model.extension.budget.power.ui.Section"
		      snippet="de.dlr.sc.virsat.model.extension.budget.power.ui.snippet.UiSnippetTablePowerState">
		</uiSnippet>
		<uiSnippet
		      id="de.dlr.sc.virsat.model.extension.budget.power.section.uiSnippetPowerState"
		      section="de.dlr.sc.virsat.model.extension.budget.power.ui.Section"
		      snippet="de.dlr.sc.virsat.model.extension.budget.power.ui.snippet.UiSnippetSectionPowerState">
		</uiSnippet>
	</extension>
