<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
	<extension point="org.eclipse.core.expressions.propertyTesters">
	 <propertyTester
	    id="testConcept.ui.propertyTester.conceptEnabledTester"
	    type="org.eclipse.emf.ecore.EObject"
	    namespace="testConcept.ui.propertyTester"
	    properties="conceptEnabled"
	    class="testConcept.propertyTester.ConceptEnabledTester">
	  </propertyTester>
	</extension>
	<extension point="de.dlr.sc.virsat.model.edit.ConceptImageContribution">
	<conceptImage
		fullQualifiedID="testConcept"
		pathToImage="resources/icons/testConcept.gif">
	</conceptImage>
	<conceptImage
		fullQualifiedID="testConcept.testCategory"
		pathToImage="resources/icons/testCategory.gif">
	</conceptImage>
	<conceptImage
		fullQualifiedID="testConcept.testStructural1"
		pathToImage="resources/icons/testStructural1.gif">
	</conceptImage>
	<conceptImage
		fullQualifiedID="testConcept.testStructural2"
		pathToImage="resources/icons/testStructural2.gif">
	</conceptImage>
	</extension>
	<extension point="org.eclipse.ui.commands">
	<command
		id="testConcept.ui.command.AddtestCategory"
		name="Add testCategory">
	</command>
	<command
		id="testConcept.ui.command.AddtestStructural1"
		name="Add testStructural1">
	</command>
	<command
		id="testConcept.ui.command.AddtestStructural2"
		name="Add testStructural2">
	</command>
	</extension>
	<extension point="org.eclipse.ui.commandImages">
	<image
	      commandId="testConcept.ui.command.AddtestCategory"
	      disabledIcon="resources/icons/testCategory_disabled.gif"
	      icon="resources/icons/testCategory.gif">
	</image>
	<image
	      commandId="testConcept.ui.command.AddtestStructural1"
	      disabledIcon="resources/icons/testStructural1_disabled.gif"
	      icon="resources/icons/testStructural1.gif">
	</image>
	<image
	      commandId="testConcept.ui.command.AddtestStructural2"
	      disabledIcon="resources/icons/testStructural2_disabled.gif"
	      icon="resources/icons/testStructural2.gif">
	</image>
	</extension>
	<extension point="org.eclipse.ui.handlers">
	<handler
		class="testConcept.ui.handler.AddTestCategoryHandler"
		commandId="testConcept.ui.command.AddtestCategory">
	</handler>
	<handler
		class="testConcept.ui.handler.AddStructuralElementInstanceTestStructural1Handler"
		commandId="testConcept.ui.command.AddtestStructural1">
	</handler>
	<handler
		class="testConcept.ui.handler.AddStructuralElementInstanceTestStructural2Handler"
		commandId="testConcept.ui.command.AddtestStructural2">
	</handler>
	</extension>
	<extension point="org.eclipse.ui.menus">
		<menuContribution
		     allPopups="false"
		     locationURI="popup:de.dlr.sc.virsat.project.ui.navigator.menu#PopupMenu?after=concepts">
		     <menu
		        id="testConcept.menu" 
		        label="testConcept"
		        icon="resources/icons/testConcept.gif"
		        tooltip="">
		        <separator
		            name="categories"
		            visible="true">
			    </separator>
		        <separator
			        name="structuralElements"
		            visible="true">
			    </separator>
		        <separator
			        name="tools"
		            visible="true">
			    </separator>
		        <separator
		        	name="additions"
		            visible="true">
		    	</separator>
		    	<visibleWhen
					checkEnabled="true">
					<with
						variable="selection">
						<iterate
							ifEmpty="false"
							operator="and">
							<test 
								property="testConcept.ui.propertyTester.conceptEnabled"
								value="true" 
								forcePluginActivation="true">
							</test>		
						</iterate>
					</with>
				</visibleWhen>
			</menu>
		</menuContribution>
		<menuContribution allPopups="false"
			locationURI="popup:testConcept.menu?after=categories">
		<command
			commandId="testConcept.ui.command.AddtestCategory"
			style="push">
			<visibleWhen
				checkEnabled="true">
				<with
					variable="selection">
					<iterate
						ifEmpty="false"
						operator="and">
						<test 
							property="testConcept.ui.propertyTester.conceptEnabled"
							value="true" 
							forcePluginActivation="true">
						</test>		
						<instanceof
							value="de.dlr.sc.virsat.model.dvlm.categories.ICategoryAssignmentContainer">
						</instanceof>
					</iterate>
				</with>
			</visibleWhen>
		</command>
		</menuContribution>
		<menuContribution allPopups="false"
			locationURI="popup:testConcept.menu?after=structuralElements">
		<command
			commandId="testConcept.ui.command.AddtestStructural1"
			style="push">
			<visibleWhen
				checkEnabled="true">
				 <with
		         	variable="selection">
		          	<iterate
		            	ifEmpty="false"
		                operator="and">
		                <test 
		              		property="testConcept.ui.propertyTester.conceptEnabled"
		              		value="true" 
		              		forcePluginActivation="true">
		                </test>	
		              	<or>
		                	<instanceof
		                       value="de.dlr.sc.virsat.model.dvlm.structural.StructuralElementInstance">
		                 	</instanceof>
		                 	<instanceof
		                       value="de.dlr.sc.virsat.model.dvlm.Repository">
		                 	</instanceof>
		              	</or>
		           	</iterate>
		           	<and>
		              <count
		                    value="1">
		              </count>
		           </and>
		        </with>
			</visibleWhen>
		</command>
		<command
			commandId="testConcept.ui.command.AddtestStructural2"
			style="push">
			<visibleWhen
				checkEnabled="true">
				 <with
		         	variable="selection">
		          	<iterate
		            	ifEmpty="false"
		                operator="and">
		                <test 
		              		property="testConcept.ui.propertyTester.conceptEnabled"
		              		value="true" 
		              		forcePluginActivation="true">
		                </test>	
		              	<or>
		                	<instanceof
		                       value="de.dlr.sc.virsat.model.dvlm.structural.StructuralElementInstance">
		                 	</instanceof>
		                 	<instanceof
		                       value="de.dlr.sc.virsat.model.dvlm.Repository">
		                 	</instanceof>
		              	</or>
		           	</iterate>
		           	<and>
		              <count
		                    value="1">
		              </count>
		           </and>
		        </with>
			</visibleWhen>
		</command>
		</menuContribution>
	</extension>
	<extension
	       point="de.dlr.sc.virsat.uiengine.ui.EditorSection">
	    <editorSection
	          id="testConcept.ui.Section"
	          topRanking="1000">
	    </editorSection>
	 </extension>	
	<extension point="de.dlr.sc.virsat.uiengine.ui.EditorUiSnippets">
		<uiSnippet
		      id="testConcept.table.uiSnippettestCategory"
		      section="testConcept.ui.Section"
		      snippet="testConcept.ui.snippet.UiSnippetTableTestCategory">
		</uiSnippet>
		<uiSnippet
		      id="testConcept.section.uiSnippettestCategory"
		      section="testConcept.ui.Section"
		      snippet="testConcept.ui.snippet.UiSnippetSectionTestCategory">
		</uiSnippet>
		<uiSnippet
		      id="testConcept.table.uiSnippettestCategoryTpSringArrayDynamic"
		      section="testConcept.ui.Section"
		      snippet="testConcept.ui.snippet.UiSnippetTableTestCategoryTpSringArrayDynamic">
		</uiSnippet>
		<uiSnippet
		      id="testConcept.table.uiSnippettestCategoryTpSringArrayStatic"
		      section="testConcept.ui.Section"
		      snippet="testConcept.ui.snippet.UiSnippetTableTestCategoryTpSringArrayStatic">
		</uiSnippet>
		<uiSnippet
		      id="testConcept.table.uiSnippettestCategoryTpIntArrayDynamic"
		      section="testConcept.ui.Section"
		      snippet="testConcept.ui.snippet.UiSnippetTableTestCategoryTpIntArrayDynamic">
		</uiSnippet>
		<uiSnippet
		      id="testConcept.table.uiSnippettestCategoryTpIntArrayStatic"
		      section="testConcept.ui.Section"
		      snippet="testConcept.ui.snippet.UiSnippetTableTestCategoryTpIntArrayStatic">
		</uiSnippet>
		<uiSnippet
		      id="testConcept.table.uiSnippettestCategoryTpFloatArrayDynamic"
		      section="testConcept.ui.Section"
		      snippet="testConcept.ui.snippet.UiSnippetTableTestCategoryTpFloatArrayDynamic">
		</uiSnippet>
		<uiSnippet
		      id="testConcept.table.uiSnippettestCategoryTpFloatArrayStatic"
		      section="testConcept.ui.Section"
		      snippet="testConcept.ui.snippet.UiSnippetTableTestCategoryTpFloatArrayStatic">
		</uiSnippet>
		<uiSnippet
		      id="testConcept.table.uiSnippettestCategoryTpBooleanArrayDynamic"
		      section="testConcept.ui.Section"
		      snippet="testConcept.ui.snippet.UiSnippetTableTestCategoryTpBooleanArrayDynamic">
		</uiSnippet>
		<uiSnippet
		      id="testConcept.table.uiSnippettestCategoryTpBooleanArrayStatic"
		      section="testConcept.ui.Section"
		      snippet="testConcept.ui.snippet.UiSnippetTableTestCategoryTpBooleanArrayStatic">
		</uiSnippet>
		<uiSnippet
		      id="testConcept.table.uiSnippettestCategoryTpResourceDynamich"
		      section="testConcept.ui.Section"
		      snippet="testConcept.ui.snippet.UiSnippetTableTestCategoryTpResourceDynamich">
		</uiSnippet>
		<uiSnippet
		      id="testConcept.table.uiSnippettestCategoryTpResourceStatic"
		      section="testConcept.ui.Section"
		      snippet="testConcept.ui.snippet.UiSnippetTableTestCategoryTpResourceStatic">
		</uiSnippet>
	</extension>
	<!-- Plugin.XML Protected Region Start -->
testExtractProtectedRegion
	<!-- Plugin.XML Protected Region End -->
</plugin>

